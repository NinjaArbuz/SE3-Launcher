{
    "name": "se3-launcher",
    "version": "3.0.0",
    "private": true,
    "homepage": "./",
    "main": "index.js",
    "author": {
        "name": "Nadwey",
        "url": "https://github.com/Nadwey/"
    },
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^7.2.1",
        "cross-env": "^7.0.3",
        "dot-env": "^0.0.1",
        "electron": "^19.0.1",
        "electron-packager": "^15.5.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "^5.0.1",
        "wait-on": "^6.0.1",
        "yarn-or-npm": "^3.0.1"
    },
    "scripts": {
        "start": "concurrently --kill-others \"cross-env BROWSER=none react-scripts start\" \"electron .\"",
        "react-build": "react-scripts build",
        "electron-build": "electron-packager . \"SE3 Launcher\" --platform=win32 --arch=ia32 --asar --overwrite --out=dist",
        "build": "yon run react-build && yon run electron-build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@6c65726f79/custom-titlebar": "^0.9.1",
        "@electron/remote": "^2.0.8",
        "@mantine/core": "^4.2.6",
        "@mantine/hooks": "^4.2.6",
        "@mantine/modals": "^4.2.6",
        "@mantine/next": "^4.2.6",
        "@mantine/notifications": "^4.2.6",
        "appdata-path": "^1.0.0",
        "axios": "^0.27.2",
        "download": "^8.0.0",
        "electron-is-dev": "^2.0.0",
        "electron-store": "^8.0.1",
        "github-markdown-css": "^5.1.0",
        "react-markdown": "^8.0.3",
        "remark-gfm": "^3.0.1"
    },
    "build": {
        "extends": null,
        "files": [
            "!**/src/*",
            "!**/resources/*",
            "!**/public/*",
            "!debugConfig.json",
            "!.prettierrc"
        ]
    }
}
